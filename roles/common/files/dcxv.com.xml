<clientConfig version="1.1">
  <emailProvider id="dcxv.com">
    <domain>dcxv.com</domain>
    <displayName>DCXV Company Email</displayName>
    <displayShortName>DCXV</displayShortName>
    <incomingServer type="imap">
      <hostname>dcxv.com</hostname>
      <port>1993</port>
      <socketType>SSL</socketType>
      <authentication>plain</authentication>
      <username>%EMAILADDRESS%</username>
    </incomingServer>
    <outgoingServer type="smtp">
      <hostname>dcxv.com</hostname>
      <port>1587</port>
      <socketType>STARTTLS</socketType>
      <authentication>plain</authentication>
      <username>%EMAILADDRESS%</username>
    </outgoingServer>
  </emailProvider>
</clientConfig>
